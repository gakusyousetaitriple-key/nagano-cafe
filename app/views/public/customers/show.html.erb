<h3>マイページ</h3>
<div class="row">
  <div class="col-3"><h3>登録情報</h3></div>
  <div class="col-2">
    <%= link_to "編集する", edit_public_customer_path(@customer), class: "btn btn-success" %>
  </div>
  <div class="col-8"></div>
</div>
<table class="table">
  <tr>
    <td><%= "氏名" %></td>
    <td><%= @customer.last_name %></td>
    <td><%= @customer.first_name %></td>
  </tr>
  <tr>
    <td><%= "カナ" %></td>
    <td><%= @customer.kana_last_name %></td>
    <td><%= @customer.kana_first_name %></td>
  </tr>
  <tr>
    <td><%= "郵便番号" %></td>
    <td><%= @customer.post_code %></td>
  </tr>
  <tr>
    <td><%= "住所" %></td>
    <td><%= @customer.address %></td>
  </tr>
  <tr>
    <td><%= "電話番号" %></td>
    <td><%= @customer.telephone_number %></td>
  </tr>
  <tr>
    <td><%= "メールアドレス" %></td>
    <td><%= @customer.email %></td>
  </tr>
</table>
<h3>配送先</h3>
<h3>注文一覧</h3>
